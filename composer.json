{
  "name": "usmanzahid/n8n-php",
  "description": "n8n-php is a lightweight PHP SDK for the n8n automation platform. It provides a fast, reliable way to connect to your n8n instance and interact with its API, making automation in PHP straightforward and efficient.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "n8n",
    "api",
    "sdk",
    "workflow",
    "automation"
  ],
  "homepage": "https://github.com/usman/n8n-php",
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.9"
  },
  "autoload": {
    "psr-4": {
      "UsmanZahid\\N8n\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UsmanZahid\\N8n\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Usman Zahid",
      "email": "developerusman@yahoo.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "^12",
    "vlucas/phpdotenv": "^5.6",
    "symfony/var-dumper": "7.4.x-dev"
  },
  "scripts": {
    "test": "phpunit"
  }
}
